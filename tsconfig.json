{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "verbatimModuleSyntax": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
      "@layouts/*": ["src/layouts/*"],
      "@utils/*": ["src/utils/*"],
      "@cms/*": ["src/cms/*"],
      "@icons/*": ["src/icons/*"],
      "@src/*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@bookImg/*": ["src/assets/images/books/*"],
      "@data/*": ["src/data/*"],
      "@locales/*": ["src/locales/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
